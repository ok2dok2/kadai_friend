<h1>blogs_new</h1>

<h1>Blogs#new</h1>

<%= form_with(model: @blog, local: true) do |form| %>
    <div class="blog_title">
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div class="blog_content">
      <%= form.label :content %>
      <%= form.text_field :content %>
    </div>
    <%= form.submit %>
  <% end %>

<p>Find me in app/views/blogs/new.html.erb</p>

<h1>Sessions#new</h1>

<h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :Eメール %>
  <%= f.email_field :email %>

  <%= f.label :パスワード %>
  <%= f.password_field :password %>

  <p><%= f.submit "ログイン" %></p>
<% end %>

<p>Find me in app/views/sessions/new.html.erb</p>
